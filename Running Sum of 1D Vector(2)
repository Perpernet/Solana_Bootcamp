fn running_sum(nums: Vec<i32>) -> Vec<i32> {
    let mut running_sums = Vec::new();
    let mut current_sum = 0;

    for num in nums {
        current_sum += num;
        running_sums.push(current_sum);
    }

    running_sums
}

fn main() {
    let nums = vec![1, 1, 1, 1, 1];
    let output = running_sum(nums);
    println!("{:?}", output); // Output: [1, 2, 3, 4, 5]
}
